// https://cses.fi/problemset/result/11900403/ 
#include <iostream> 
#include <vector> 
#include <climits> 
using namespace std; 
int n, x; 
// const int mod = 1000000007; 
int main() { 
    ios_base::sync_with_stdio(false); cin.tie(0); cout.tie(0); 
    cin >> n >> x; 
    vector<int> v(n), dp(1000001, INT_MAX); 
    for (int i = 0; i < n; i++) {
        cin >> v[i]; 
        dp[v[i]] = 1; 
    }  
    for (int i = 1; i <= x; i++) {  
        for (int j : v) {   
            if (i - j >= 0 && dp[i - j] != INT_MAX) dp[i] = min(dp[i], 1 + dp[i - j]); 
        } 
    } 
    if (dp[x] == INT_MAX) cout << -1; 
    else cout << dp[x]; 
}  
